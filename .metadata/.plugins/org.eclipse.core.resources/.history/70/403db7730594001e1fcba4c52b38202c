package com.day3;

class Person {
	private char gender ;
	
	private int age ;
	public Person(char gender, int age) {
//		super();
		this.gender = gender;
		this.age = age;
	}
	
	@Override
	public String toString() {
		return "Person [gender=" + gender + ", age=" + age + ", getClass()=" + getClass() + ", hashCode()=" + hashCode()
				+ ", toString()=" + super.toString() + "]";
	}

	public char getGender() {
		return gender;
	}
	public void setGender(char gender) {
		this.gender = gender;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	
}


public class JavaBean {

	public static void main(String[] args) {
		
		Person baby = new Person ('M',1);
		System.out.println("Gender is :"+baby.getGender()); // gen MAle
		
		System.out.println("Age is :"+baby.getAge()); // age 1
		
		baby.setAge(2);  // after set 2
		
        System.out.println("Gender is :"+baby.getGender()); // Gen Male
		
		System.out.println("Age is :"+baby.getAge()); // Age 2
		System.out.println("***************************************");
System.out.println(baby);
	}

}
